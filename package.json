{
  "name": "e_commerce_sample",
  "version": "1.0.0",
  "description": "It's a sample of an e-commerce.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build:prod": "webpack --config webpack/webpack.base.js -p",
    "dev": "concurrently \"npm run watch:dev\" \"npm run build:dev\"",
    "watch:dev": "nodemon index.js",
    "build:dev": "webpack --watch --config webpack/webpack.dev.js",
    "heroku-postbuild": "npm run build:prod",
    "lint:fix": "standard --env mocha --fix",
    "test": "mocha-webpack --webpack-config webpack/webpack.dev.js test --recursive --require tests/.setup.js"
  },
  "engines": {
    "node": "~6.0.0",
    "npm": "~4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrunoCasotto/e_commerce_sample.git"
  },
  "keywords": [
    "e-commerce",
    "node",
    "sample",
    "commerce"
  ],
  "author": "Bruno Casotto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrunoCasotto/e_commerce_sample/issues"
  },
  "homepage": "https://github.com/BrunoCasotto/e_commerce_sample#readme",
  "devDependencies": {
    "avoriaz": "^1.14.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^4.0.0",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^3.2.0",
    "mocha-webpack": "next",
    "standard": "^10.0.2",
    "webpack-node-externals": "^1.6.0",
    "deepmerge": "^1.4.3",
    "concurrently": "^2.2.0",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "autoprefixer": "^6.7.3",
    "axios": "^0.12.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "growl-alert": "^0.3.2",
    "hapi": "^16.0.1",
    "inert": "^4.0.3",
